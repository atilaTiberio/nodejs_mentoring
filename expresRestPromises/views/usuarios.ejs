<% include _headers %>

<h2>Usuarios</h2>
<div style="align: center;">
    <a class="btn btn-primary" href="<%=contextPath%>/alta"><span class="glyphicon glyphicon-plus-sign"></span> Alta de Usuario</a>

<table class="table"><thead><th>username</th><th>password</th><th>email</th><th>Phone</th><th colspan="2">Operacion</th></thead>
<tbody>
    <% data.forEach(function(item){%>

        <tr>

            <td><%=item.name%></td>
            <td><%=item.password%></td>
            <td><%=item.email%></td>
            <td><%=item.phone%></td>

        <td><a class="btn btn-danger" href="<%=contextPath%>/delete/<%=item.id%>" >Borrar</a></td>
        <td><a class="btn btn-primary" href="<%=contextPath%>/actualizar/<%=item.id%>" >Actualizar</a></td></tr>
    <%});%>

</tbody>

</table>

</div>
</body>


</html>

<% include _footer %>
